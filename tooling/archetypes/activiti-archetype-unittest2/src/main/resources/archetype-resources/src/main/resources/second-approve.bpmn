        <omgdi:waypoint x="282.0" y="310.0"></omgdi:waypoint>
        <omgdi:waypoint x="282.0" y="215.0"></omgdi:waypoint>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge bpmnElement="flow10" id="BPMNEdge_flow10">
        <omgdi:waypoint x="870.0" y="170.0"></omgdi:waypoint>
